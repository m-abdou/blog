{% extends "::base.html.twig" %}
  {% block stylesheets %}
      <link href="{{ asset("bundles/app/bootstrap/css/bootstrap.min.css")}}"  media="all" rel="stylesheet" type="text/css">
      <link href="{{ asset("bundles/app/bootstrap/css/bootstrap-responsive.min.css")}}" media="all" rel="stylesheet" type="text/css">
      <link href="{{ asset("bundles/app/assets/styles.css")}}" media="all" rel="stylesheet" type="text/css">
  {% endblock %}

  {% block body %}
      <div class="navbar navbar-fixed-top">
          <div class="navbar-inner">
              <div class="container-fluid">
                  <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"> <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                  </a>
                  <a class="brand" href="#">Admin Panel</a>
              </div>
          </div>
      </div>
      <div class="container-fluid">
          <div class="row-fluid">
              <div class="span3" id="sidebar">
                  {% block slidbar %}
                  {% endblock %}
              </div>
              <div class="span9" id="content">
                  {% block content %}
                  {% endblock %}
              </div>
          </div>
      </div>
  {% endblock %}
  {% block javascripts %}
      <script src="{{ asset("bundles/app/vendors/jquery-1.9.1.js") }}" type="text/javascript"></script>
      <script src="{{ asset("bundles/app/bootstrap/js/bootstrap.min.js") }}" type="text/javascript"></script>
      <script src="{{ asset("bundles/app/vendors/jquery.uniform.min.js") }}" type="text/javascript"></script>
      <script src="{{ asset("bundles/app/vendors/chosen.jquery.min.js") }}" type="text/javascript"></script
      <script src="{{ asset("bundles/app/vendors/bootstrap-datepicker.js") }}" type="text/javascript"></script>
      <script src="{{ asset("bundles/app/vendors/wysiwyg/wysihtml5-0.3.0.js") }}" type="text/javascript"></script>
      <script src="{{ asset("bundles/app/vendors/wysiwyg/bootstrap-wysihtml5.js") }}" type="text/javascript"></script>
      <script src="{{ asset("bundles/app/vendors/wizard/jquery.bootstrap.wizard.min.js") }}" type="text/javascript"></script>
  {% endblock %}